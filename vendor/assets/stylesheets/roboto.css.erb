@font-face {
  font-family: 'RobotoDraft';
  font-style: normal;
  font-weight: 400;
  src: local('RobotoDraft'), local('RobotoDraft-Regular'), local('Roboto-Regular'),
  url('<%= font_path('RobotoDraftRegular.woff2') %>') format('woff2'),
  url('<%= font_path('RobotoDraftRegular.woff') %>') format('woff');
}
@font-face {
  font-family: 'RobotoDraft';
  font-style: normal;
  font-weight: 500;
  src: local('RobotoDraft Medium'), local('RobotoDraft-Medium'), local('Roboto-Medium'),
  url('<%= font_path('RobotoDraftMedium.woff2') %>') format('woff2'),
  url('<%= font_path('RobotoDraftMedium.woff') %>') format('woff');
}
@font-face {
  font-family: 'RobotoDraft';
  font-style: normal;
  font-weight: 700;
  src: local('RobotoDraft Bold'), local('RobotoDraft-Bold'), local('Roboto-Bold'),
  url('<%= font_path('RobotoDraftBold.woff2') %>') format('woff2'),
  url('<%= font_path('RobotoDraftBold.woff') %>') format('woff');
}
@font-face {
  font-family: 'RobotoDraft';
  font-style: italic;
  font-weight: 400;
  src: local('RobotoDraft Italic'), local('RobotoDraft-Italic'), local('Roboto-Italic'),
  url('<%= font_path('RobotoDraftItalic.woff2') %>') format('woff2'),
  url('<%= font_path('RobotoDraftItalic.woff') %>') format('woff');
}
